<h1> You are in studio new view</h1>

<H2> Welcome to this studio</H2>
<%= form_for @studio do |f| %>
<%= f.label :artist_id, "Artist"  %>
<%= f.collection_select :artist_id, @artist, :id, :name %> <br>
<%= f.label :instrument_id, "Instrument"  %>
<%= f.collection_select :instrument_id, Instrument.all, :id, :name %> <br>
<%= f.submit %>
<% end %>
