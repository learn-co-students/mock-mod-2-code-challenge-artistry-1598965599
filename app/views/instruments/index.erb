<div class="container">
<div class="row justify-content-center m-5">
<h1> All the Instruments </h1>
</div>

    <div class="row justify-content-center">
        <ul class="list-group">
            <% @instruments.each do |instrument| %>
            <li class="list-group-item">
                <%= instrument.name %> - <%= instrument.classification%>
            </li>
            <% end %>
        </ul>
    </div>

</div>